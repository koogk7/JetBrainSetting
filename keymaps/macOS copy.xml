<keymap version="1" name="macOS copy" parent="Mac OS X 10.5+">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="shift meta comma" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="meta open_bracket" />
    <keyboard-shortcut first-keystroke="meta alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="shift alt comma" />
  </action>
  <action id="EditorBackwardParagraph">
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
  <action id="EditorBackwardParagraphWithSelection">
    <keyboard-shortcut first-keystroke="shift alt k" />
  </action>
  <action id="EditorCloneCaretAbove">
    <keyboard-shortcut first-keystroke="shift ctrl up" />
  </action>
  <action id="EditorCloneCaretBelow">
    <keyboard-shortcut first-keystroke="shift ctrl down" />
  </action>
  <action id="EditorForwardParagraph">
    <keyboard-shortcut first-keystroke="alt j" />
  </action>
  <action id="EditorForwardParagraphWithSelection">
    <keyboard-shortcut first-keystroke="shift alt j" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="meta close_bracket" />
    <keyboard-shortcut first-keystroke="meta alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="shift alt period" />
  </action>
  <action id="Git.Branches">
    <keyboard-shortcut first-keystroke="alt b" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="meta back_slash" />
  </action>
</keymap>